{
  "name": "@todo-example/modules",
  "private": true,
  "version": "11.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "browserslist": {
    "production": [
      "defaults",
      "> 0.2%",
      "not ie < 12",
      "not safari < 10",
      "not ios_saf < 10"
    ]
  },
  "main": "./build/index-node18.mjs",
  "module": "./build/index-browser.es.js",
  "browser": "./build/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "node": {
        "import": "./build/index-node18.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./build/index-browsermodern.es.js"
        },
        "import": "./build/index-browser.es.js"
      }
    }
  },
  "sideEffects": true,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet @todo-example/modules",
    "start": "yarn clean:build && rollup --config rollup.config.mjs --watch",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "18"
      },
      {
        "target": "browser",
        "version": "modern"
      },
      {
        "target": "browser"
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "liwi-mongo": "11.0.0",
    "liwi-resources-client": "11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/preset-env": "7.22.20",
    "pob-babel": "36.2.0",
    "typescript": "5.2.2"
  }
}
