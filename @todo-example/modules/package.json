{
  "name": "@todo-example/modules",
  "private": true,
  "version": "9.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./build/index-node14.mjs",
  "module": "./build/index-browser.es.js",
  "browser": "./build/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./build/index-node14.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./build/index-browsermodern.es.js"
        },
        "import": "./build/index-browser.es.js"
      }
    }
  },
  "module:node": "./build/index-node14.mjs",
  "module:modern-browsers": "./build/index-browsermodern.es.js",
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs",
    "clean": "yarn clean:build",
    "clean:build": "rm -Rf build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet @todo-example/modules",
    "start": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "liwi-mongo": "9.0.0",
    "liwi-resources-client": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "32.2.0",
    "typescript": "4.6.3"
  }
}
