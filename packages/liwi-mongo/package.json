{
  "name": "liwi-mongo",
  "version": "7.0.0",
  "description": "mongo implementation for liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:liwijs/liwi.git",
    "directory": "packages/liwi-mongo"
  },
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --quiet packages/liwi-mongo",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@types/mongodb": "^3.5.20",
    "liwi-store": "^7.0.0",
    "liwi-subscribe-store": "^7.0.0",
    "liwi-types": "^7.0.0",
    "mingo": "^3.0.2",
    "mongodb": "^3.5.8",
    "nightingale-logger": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "babel-preset-latest-node": "4.1.0",
    "pob-babel": "24.14.0"
  }
}
