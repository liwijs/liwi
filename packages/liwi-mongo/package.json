{
  "name": "liwi-mongo",
  "version": "5.0.4",
  "description": "mongo implementation for liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:liwijs/liwi.git",
    "directory": "packages/liwi-mongo"
  },
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"]
  },
  "dependencies": {
    "@types/mongodb": "^3.3.1",
    "liwi-store": "^4.3.2",
    "liwi-subscribe-store": "^2.4.0",
    "liwi-types": "^3.0.2",
    "mingo": "^2.3.5",
    "mongodb": "^3.3.2",
    "nightingale-logger": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@pob/eslint-config-babel-node": "32.0.13",
    "@pob/eslint-config-typescript": "32.0.13",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.3.0",
    "pob-babel": "23.20.0",
    "prettier": "1.18.2"
  }
}
