    "@typescript-eslint/parser": "1.2.0",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "3.0.1",
    "babel-preset-modern-browsers": "13.0.1",
    "eslint": "5.13.0",
    "eslint-config-pob": "26.0.8",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "pob-babel": "23.6.1",
    "prettier": "1.16.4",
    "typescript": "3.3.1"
  }
