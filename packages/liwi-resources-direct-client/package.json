{
  "name": "liwi-resources-direct-client",
  "version": "7.5.0",
  "description": "direct client implementation for liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:liwijs/liwi.git",
    "directory": "packages/liwi-resources-direct-client"
  },
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --quiet packages/liwi-resources-direct-client",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "extended-json": "^7.0.0",
    "liwi-resources-client": "^7.5.0",
    "liwi-resources-server": "^7.4.3",
    "nightingale-logger": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@types/backo2": "^1.0.1",
    "babel-preset-latest-node": "4.1.0",
    "pob-babel": "24.14.0",
    "type-fest": "^0.15.0"
  }
