{
  "name": "liwi-resources-websocket-server",
  "version": "3.3.5",
  "description": "liwi resources client implementation for websocket",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:liwijs/liwi.git",
    "directory": "packages/liwi-resources-websocket-server"
  },
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .js,.ts --quiet src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index"]
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@types/socket.io": "^2.1.2",
    "extended-json": "^2.3.1",
    "liwi-resources-server": "^4.3.5",
    "liwi-store": "^4.4.0",
    "liwi-types": "^3.0.2",
    "nightingale-logger": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@pob/eslint-config-typescript": "33.0.8",
    "babel-preset-latest-node": "4.0.1",
    "babel-preset-modern-browsers": "14.1.1",
    "eslint": "6.8.0",
    "pob-babel": "24.3.0",
    "prettier": "1.19.1"
  }
}
