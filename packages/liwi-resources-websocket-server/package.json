{
  "name": "liwi-resources-websocket-server",
  "version": "8.3.1",
  "description": "liwi resources client implementation for websocket",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/liwijs/liwi.git",
    "directory": "packages/liwi-resources-websocket-server"
  },
  "homepage": "https://github.com/liwijs/liwi",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs",
        "require": "./dist/index-node14.cjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "yarn clean:build",
    "clean:build": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/liwi-resources-websocket-server",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "extended-json": "^8.3.1",
    "liwi-resources-server": "^8.3.1",
    "liwi-store": "^8.3.1",
    "liwi-types": "^8.0.4",
    "nightingale-logger": "^12.1.4",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@types/ws": "8.2.2",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "32.1.0",
    "typescript": "4.5.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.4"
  }
}
