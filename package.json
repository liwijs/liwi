{
  "name": "liwi",
  "version": "0.17.0",
  "description": "Liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:liwijs/liwi.git",
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "module": "./lib-module/index.js",
  "module-dev": "./lib-module-dev/index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/index.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/index.js",
  "module:aliases-node": {
    "./extended-json.js": "./lib-module-node8/extended-json.js",
    "./mongo.js": "./lib-module-node8/mongo.js",
    "./react-redux.js": "./lib-module-node8/react-redux.js",
    "./rest.js": "./lib-module-node8/rest.js",
    "./rest-websocket.js": "./lib-module-node8/rest-websocket.js",
    "./rethinkdb.js": "./lib-module-node8/rethinkdb.js",
    "./websocket-client.js": "./lib-module-node8/websocket-client.js"
  },
  "module:aliases-node-dev": {
    "./extended-json.js": "./lib-module-node8-dev/extended-json.js",
    "./mongo.js": "./lib-module-node8-dev/mongo.js",
    "./react-redux.js": "./lib-module-node8-dev/react-redux.js",
    "./rest.js": "./lib-module-node8-dev/rest.js",
    "./rest-websocket.js": "./lib-module-node8-dev/rest-websocket.js",
    "./rethinkdb.js": "./lib-module-node8-dev/rethinkdb.js",
    "./websocket-client.js": "./lib-module-node8-dev/websocket-client.js"
  },
  "module:aliases-browser": {
    "./extended-json.js": "./lib-module/extended-json.js",
    "./mongo.js": "./lib-module/mongo.js",
    "./react-redux.js": "./lib-module/react-redux.js",
    "./rest.js": "./lib-module/rest.js",
    "./rest-websocket.js": "./lib-module/rest-websocket.js",
    "./rethinkdb.js": "./lib-module/rethinkdb.js",
    "./websocket-client.js": "./lib-module/websocket-client.js"
  },
  "module:aliases-browser-dev": {
    "./extended-json.js": "./lib-module-dev/extended-json.js",
    "./mongo.js": "./lib-module-dev/mongo.js",
    "./react-redux.js": "./lib-module-dev/react-redux.js",
    "./rest.js": "./lib-module-dev/rest.js",
    "./rest-websocket.js": "./lib-module-dev/rest-websocket.js",
    "./rethinkdb.js": "./lib-module-dev/rethinkdb.js",
    "./websocket-client.js": "./lib-module-dev/websocket-client.js"
  },
  "module:aliases-modern-browsers": {
    "./extended-json.js": "./lib-module-modern-browsers/extended-json.js",
    "./mongo.js": "./lib-module-modern-browsers/mongo.js",
    "./react-redux.js": "./lib-module-modern-browsers/react-redux.js",
    "./rest.js": "./lib-module-modern-browsers/rest.js",
    "./rest-websocket.js": "./lib-module-modern-browsers/rest-websocket.js",
    "./rethinkdb.js": "./lib-module-modern-browsers/rethinkdb.js",
    "./websocket-client.js": "./lib-module-modern-browsers/websocket-client.js"
  },
  "module:aliases-modern-browsers-dev": {
    "./extended-json.js": "./lib-module-modern-browsers-dev/extended-json.js",
    "./mongo.js": "./lib-module-modern-browsers-dev/mongo.js",
    "./react-redux.js": "./lib-module-modern-browsers-dev/react-redux.js",
    "./rest.js": "./lib-module-modern-browsers-dev/rest.js",
    "./rest-websocket.js": "./lib-module-modern-browsers-dev/rest-websocket.js",
    "./rethinkdb.js": "./lib-module-modern-browsers-dev/rethinkdb.js",
    "./websocket-client.js": "./lib-module-modern-browsers-dev/websocket-client.js"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "NODE_ENV=production rm -Rf coverage/ ; jest --coverage --coverageReporters=lcov --coverageDirectory=coverage/",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "**/src/**/?(*.)test.js?(x)",
      "**/__tests__/**/*.test.js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js?(x)"
    ]
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "flow-runtime": "^0.10.0",
    "mongodb": "^2.2.30",
    "nightingale-logger": "^3.4.0",
    "rethinkdbdash": "^2.3.29"
  },
  "devDependencies": {
    "alp-react-redux": "^10.1.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest-node": "^0.3.1",
    "babel-preset-modern-browsers": "^9.0.2",
    "babel-preset-pob-react": "^0.2.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-pob": "^14.1.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.2",
    "pob-babel": "^17.6.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "xunit-file": "1.0.0"
  },
  "peerDependencies": {
    "alp-react-redux": "^10.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4"
  }
}
