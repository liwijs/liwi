{
  "name": "liwi",
  "version": "0.18.1",
  "description": "Liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:liwijs/liwi.git",
  "homepage": "https://github.com/liwijs/liwi",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "module": "./lib-module/index.js",
  "module-dev": "./lib-module-dev/index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/index.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/index.js",
  "module:aliases-node": {
    "./extended-json.js": "./lib-module-node8/extended-json/index.js",
    "./mongo.js": "./lib-module-node8/mongo/index.js",
    "./react-redux.js": "./lib-module-node8/react-redux/index.js",
    "./rest.js": "./lib-module-node8/rest/index.js",
    "./rest-websocket.js": "./lib-module-node8/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module-node8/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module-node8/websocket-client/index.js"
  },
  "module:aliases-node-dev": {
    "./extended-json.js": "./lib-module-node8-dev/extended-json/index.js",
    "./mongo.js": "./lib-module-node8-dev/mongo/index.js",
    "./react-redux.js": "./lib-module-node8-dev/react-redux/index.js",
    "./rest.js": "./lib-module-node8-dev/rest/index.js",
    "./rest-websocket.js": "./lib-module-node8-dev/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module-node8-dev/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module-node8-dev/websocket-client/index.js"
  },
  "module:aliases-browser": {
    "./extended-json.js": "./lib-module/extended-json/index.js",
    "./mongo.js": "./lib-module/mongo/index.js",
    "./react-redux.js": "./lib-module/react-redux/index.js",
    "./rest.js": "./lib-module/rest/index.js",
    "./rest-websocket.js": "./lib-module/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module/websocket-client/index.js"
  },
  "module:aliases-browser-dev": {
    "./extended-json.js": "./lib-module-dev/extended-json/index.js",
    "./mongo.js": "./lib-module-dev/mongo/index.js",
    "./react-redux.js": "./lib-module-dev/react-redux/index.js",
    "./rest.js": "./lib-module-dev/rest/index.js",
    "./rest-websocket.js": "./lib-module-dev/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module-dev/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module-dev/websocket-client/index.js"
  },
  "module:aliases-modern-browsers": {
    "./extended-json.js": "./lib-module-modern-browsers/extended-json/index.js",
    "./mongo.js": "./lib-module-modern-browsers/mongo/index.js",
    "./react-redux.js": "./lib-module-modern-browsers/react-redux/index.js",
    "./rest.js": "./lib-module-modern-browsers/rest/index.js",
    "./rest-websocket.js": "./lib-module-modern-browsers/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module-modern-browsers/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module-modern-browsers/websocket-client/index.js"
  },
  "module:aliases-modern-browsers-dev": {
    "./extended-json.js": "./lib-module-modern-browsers-dev/extended-json/index.js",
    "./mongo.js": "./lib-module-modern-browsers-dev/mongo/index.js",
    "./react-redux.js": "./lib-module-modern-browsers-dev/react-redux/index.js",
    "./rest.js": "./lib-module-modern-browsers-dev/rest/index.js",
    "./rest-websocket.js": "./lib-module-modern-browsers-dev/rest-websocket/index.js",
    "./rethinkdb.js": "./lib-module-modern-browsers-dev/rethinkdb/index.js",
    "./websocket-client.js": "./lib-module-modern-browsers-dev/websocket-client/index.js"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "NODE_ENV=production rm -Rf coverage/ ; jest --coverage --coverageReporters=lcov --coverageDirectory=coverage/",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "**/src/**/?(*.)test.js?(x)",
      "**/__tests__/**/*.test.js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js?(x)"
    ]
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "flow-runtime": "0.10.0",
    "mongodb": "^2.2.30",
    "nightingale-logger": "^3.4.0",
    "rethinkdbdash": "^2.3.29"
  },
  "devDependencies": {
    "alp-react-redux": "^12.1.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-modern-browsers": "^10.0.0",
    "babel-preset-pob-react": "^0.2.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-pob": "^15.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.2",
    "pob-babel": "^18.1.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-beta.5",
    "xunit-file": "1.0.0"
  },
  "peerDependencies": {
    "alp-react-redux": "^12.1.0",
    "prop-types": "^15.5.10",
    "react": "15.5.4 || ^16.0.0-beta.5"
  }
}
