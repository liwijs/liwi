{
  "name": "liwi",
  "version": "0.7.2",
  "description": "Liwi",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:liwijs/liwi.git",
  "homepage": "https://github.com/liwijs/liwi",
  "main": "./lib/index.js",
  "webpack:main": "./lib-webpack/index.js",
  "webpack:main-dev": "./lib-webpack-dev/index.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/index.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf coverage/ ; node --harmony --es_staging node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha -- --recursive --reporter=spec -u tdd test/node6",
    "lint": "eslint --ext .js,.jsx --fix src/ test/src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "mocha --harmony --es_staging --recursive --bail -u tdd test/node6",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "mongodb": "^2.2.10",
    "msgpack5": "^3.4.0",
    "nightingale-logger": "^1.3.0",
    "react": "^15.3.2",
    "rethinkdbdash": "^2.3.23"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^5.1.0",
    "babel-preset-modern-browsers-stage-1": "^1.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb-base": "^7.2.0",
    "eslint-config-pob": "^7.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-import": "^1.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "pob-babel": "^8.0.4",
    "pob-release": "^2.1.2",
    "tcomb-forked": "^3.3.0",
    "xunit-file": "1.0.0"
  },
  "webpack:aliases": {
    "./websocket-client.js": "./lib-webpack/websocket-client/index.js",
    "./react-redux.js": "./lib-webpack/react-redux/index.js"
  },
  "webpack:aliases-dev": {
    "./websocket-client.js": "./lib-webpack-dev/websocket-client/index.js",
    "./react-redux.js": "./lib-webpack-dev/react-redux/index.js"
  },
  "webpack:aliases-modern-browsers": {
    "./websocket-client.js": "./lib-webpack-modern-browsers/websocket-client/index.js",
    "./react-redux.js": "./lib-webpack-modern-browsers/react-redux/index.js"
  },
  "webpack:aliases-modern-browsers-dev": {
    "./websocket-client.js": "./lib-webpack-modern-browsers-dev/websocket-client/index.js",
    "./react-redux.js": "./lib-webpack-modern-browsers-dev/react-redux/index.js"
  }
}
